table '{{ table.name }}'
  lineageTag: {{ generate_lineage_tag() }}

# Calculation group artifact
{% if table.columns %}
{% for col in table.columns %}
  column '{{ col.name }}'
    dataType: {{ col.dataType }}
    lineageTag: {{ generate_lineage_tag() }}
{% endfor %}
{% endif %}
{% if table.measures %}
{% for m in table.measures %}
  measure '{{ m.name }}' = ```
{{ m.expression }}
```
    lineageTag: {{ generate_lineage_tag() }}
{% if m.formatString %}
    formatString: {{ m.formatString }}
{% endif %}
{% endfor %}
{% else %}
  # No measures defined for calculation group
{% endif %}
